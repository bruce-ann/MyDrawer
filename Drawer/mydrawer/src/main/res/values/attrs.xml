<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="direction">
        <enum name="left" value="0" />
        <enum name="right" value="1" />
        <enum name="top" value="2" />
        <enum name="bottom" value="3" />
    </attr>
    <declare-styleable name="DrawerLayout">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="direction" />
    </declare-styleable>
    <declare-styleable name="DragLayout">
        <attr name="drag_left" format="reference" />
        <attr name="drag_right" format="reference" />
        <attr name="drag_top" format="reference" />
        <attr name="drag_bottom" format="reference" />
    </declare-styleable>
</resources>
